<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 4.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Student">   
	
	<select id="getStudentDetails" parameterType="java.lang.String" resultType="map">
		select * from student.student_data where pin = #{pin};
	</select>
	     	  
  
</mapper>        